{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "carsalesdatafactory1"
		},
		"LS_AzureSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSQL'"
		},
		"LS_DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLake'"
		},
		"LS_AzureSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "saleserver1.database.windows.net"
		},
		"LS_AzureSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salessqldb"
		},
		"LS_AzureSQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminrutu"
		},
		"LS_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salesstorageacc.dfs.core.windows.net/"
		},
		"LS_GIT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "carsalesdatafactory1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSQL_password": {
						"value": "[parameters('LS_AzureSQL_password')]"
					},
					"LS_DataLake_accountKey": {
						"value": "[parameters('LS_DataLake_accountKey')]"
					},
					"LS_AzureSQL_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQL_properties_typeProperties_server')]"
					},
					"LS_AzureSQL_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQL_properties_typeProperties_database')]"
					},
					"LS_AzureSQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQL_properties_typeProperties_userName')]"
					},
					"LS_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLake_properties_typeProperties_url')]"
					},
					"LS_GIT_properties_typeProperties_url": {
						"value": "[parameters('LS_GIT_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}